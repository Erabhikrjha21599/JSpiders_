package assignment_1_1;

/**
 * 
 */
public class Prog_1_5 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {1, 2, 3, 4};
		
		for(int i = 0; i < arr.length; i++) {
			if (arr[i] == arr[i]) {
				if (arr[i] != arr[i]) {
					arr[i]*= arr[i];
				}
			}
		}
		
		for(int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}

	}

}
